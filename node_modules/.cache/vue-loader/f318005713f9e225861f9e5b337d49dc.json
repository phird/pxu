\thandleSubmit(e) {\r\n\t\t\t\te.preventDefault();\r\n\t\t\t\tthis.form.validateFields((err, values) => {\r\n\t\t\t\t\tif ( !err ) {\r\n\t\t\t\t\t\tconsole.log('Received values of form: ', values) ;\r\n\t\t\t\t\t}\r\n\t\t\t\t});\r\n\t\t\t},\r\n\t\t},\r\n\t})\r\n\r\n</script>\r\n\r\n<style lang=\"scss\">\r\n\tbody {\r\n\t\tbackground-color: #ffffff;\r\n\t}\r\n</style>"]}]}