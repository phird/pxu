ection -->\r\n      <!---  Profile-detail section -->\r\n      <div class=\"card-body\">\r\n        <!-- card body section -->\r\n        <!-- 1 -->\r\n\r\n        <div class=\"content-card-body\">\r\n          <div class=\"content-box\">\r\n            <div id=\"username\">\r\n              <span style=\"font-size: 20px\">{{ em.employeeName }}</span>\r\n            </div>\r\n            <div id=\"email\">\r\n              <span\r\n                class=\"email-text\"\r\n                style=\"font-size: 16px; font-weight: 300\"\r\n                >{{ em.employeeEmail }}</span\r\n              >\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n      <!--/card body section -->\r\n      <!---/Profile-detail section -->\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from \"axios\";\r\nexport default {\r\n  data() {\r\n    return {\r\n      em: [],\r\n    };\r\n  },\r\n  created() {\r\n    this.getem();\r\n  },\r\n  methods: {\r\n    getem() {\r\n      axios.get(\"https://pxu-server.herokuapp.com/carddash\", {}).then((res) => {\r\n        this.em = res.data[0];\r\n      });\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.card-profile {\r\n  font-family: \"Mitr\", sans-serif;\r\n  position: relative;\r\n  height: 252px;\r\n  border-radius: 12px;\r\n}\r\n\r\n.card-profile-section {\r\n  /* div that contain banner and text-section */\r\n  display: flex;\r\n  position: relative;\r\n  flex-direction: column;\r\n  width: 100%;\r\n  height: 100%;\r\n}\r\n\r\n.card-profile-section .profile-banner {\r\n  /*  whole banner div*/\r\n  display: flex;\r\n  width: 100%;\r\n  height: 50%;\r\n  object-fit: cover;\r\n}\r\n.profile-banner img {\r\n  width: 100%;\r\n  height: 100%;\r\n  object-fit: cover;\r\n}\r\n.card-profile-section .card-body {\r\n  /*  whole text-section   */\r\n  display: flex;\r\n  flex-shrink: 1;\r\n  flex-direction: column;\r\n  font-size: 24px;\r\n  justify-content: center;\r\n  align-items: c