A;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA","file":"Customer.vue","sourceRoot":"src/views","sourcesContent":["<template>\r\n  <!-- Projects Table Column -->\r\n  <div class=\"pagebody\">\r\n    <div class=\"pagebody-upper\">\r\n      <!-- Counter Widgets -->\r\n      <!-- Charts -->\r\n      <a-row :gutter=\"24\" type=\"flex\" align=\"stretch\">\r\n        <a-col :span=\"24\" :lg=\"8\" class=\"mb-24\">\r\n          <!-- Active Users Card -->\r\n          <!-- <CardBarChart></CardBarChart> -->\r\n          <RecentlyAdded></RecentlyAdded>\r\n          <!-- Active Users Card -->\r\n        </a-col>\r\n        <a-col :span=\"24\" :lg=\"8\" class=\"mb-24\">\r\n          <!-- Active Users Card -->\r\n          <!-- <CardBarChart></CardBarChart> -->\r\n          <NewCustomer></NewCustomer>\r\n          <!-- Active Users Card -->\r\n        </a-col>\r\n        <a-col :span=\"24\" :lg=\"8\" class=\"mb-24\">\r\n          <!-- Sales Overview Card -->\r\n          <TypeOfCus></TypeOfCus>\r\n          <!-- / Sales Overview Card -->\r\n        </a-col>\r\n      </a-row>\r\n    </div>\r\n    <div class=\"pagebody-lower\">\r\n      <a-card\r\n        :bordered=\"false\"\r\n        class=\"header-solid h-full\"\r\n        :bodyStyle=\"{ padding: 0 }\"\r\n      >\r\n        <template #title>\r\n          <a-row type=\"flex\" align=\"middle\">\r\n            <a-col :span=\"24\" :md=\"12\">\r\n              <h5 class=\"font-semibold m-0\">ลูกค้า</h5>\r\n            </a-col>\r\n            <a-col\r\n              :span=\"24\"\r\n              :md=\"12\"\r\n              style=\"\r\n                display: flex;\r\n                align-items: center;\r\n                justify-content: flex-end;\r\n              \"\r\n            >\r\n              <div class=\"table-upload-btn\" style=\"color:white\">\r\n                <a-tooltip>\r\n              <template slot=\"title\"> เพิ่มลูกค้า </template>\r\n                <a-button type=\"primary\" @click=\"showModal\">\r\n                  <b-icon icon=\"plus-square-dotted\"></b-icon>\r\n                  Create Customer\r\n                </a-button>\r\n              </div>\r\n            </a-col>\r\n          </a-row>\r\n        </template>\r\n        <a-table\r\n          class=\"quotation-table\"\r\n          :columns=\"columns\"\r\n          :data-source=\"customer\"\r\n          :pagination=\"true\"\r\n        >\r\n          <template slot=\"companyName\" slot-scope=\"text\">\r\n            <a>{{ text }}</a>\r\n          </template>\r\n\r\n          <a-space\r\n            slot=\"index\"\r\n            slot-scope=\"index\"\r\n            :size=\"-12\"\r\n            class=\"avatar-chips\"\r\n          >\r\n            <a>{{ index }}</a>\r\n          </a-space>\r\n\r\n          <template slot=\"status\" slot-scope=\"status\">\r\n            <h6 class=\"m-0\">\r\n              {{ status }}\r\n            </h6>\r\n          </template>\r\n\r\n          <template slot=\"contactName\" slot-scope=\"contactName\">\r\n            <p>{{ contactName }}</p>\r\n          </template>\r\n\r\n          <template slot=\"contactNumber\" slot-scope=\"contactNumber\">\r\n            {{ contactNumber }}\r\n          </template>\r\n\r\n          <template slot=\"actionSection\" slot-scope=\"customerID\">\r\n            <a-dropdown>\r\n              <a class=\"ant-dropdown-link\" @click=\"(e) => e.preventDefault()\">\r\n                <svg\r\n                  xmlns=\"http://www.w3.org/2000/svg\"\r\n                  width=\"16\"\r\n                  height=\"16\"\r\n                  fill=\"currentColor\"\r\n                  class=\"bi bi-three-dots-vertical\"\r\n                  viewBox=\"0 0 16 16\"\r\n                >\r\n                  <path\r\n                    d=\"M9.5 13a1.5 1.5 0 1 1-3 0 1.5 1.5 0 0 1 3 0zm0-5a1.5 1.5 0 1 1-3 0 1.5 1.5 0 0 1 3 0zm0-5a1.5 1.5 0 1 1-3 0 1.5 1.5 0 0 1 3 0z\"\r\n                  />\r\n                </svg>\r\n              </a>\r\n              <a-menu slot=\"overlay\">\r\n                <a-menu-item>\r\n                  <router-link\r\n                    :to=\"`/Customers/EditCustomer/${customerID}`\"\r\n                    style=\"text-decoration: none\"\r\n                  >\r\n                    <a style=\"text-decoration: none\"> แก้ไข </a>\r\n                  </router-link>\r\n                </a-menu-item>\r\n\r\n                <a-menu-item>\r\n                  <a\r\n                    style=\"text-decoration: none\"\r\n                    @click=\"deletecus(customerID)\"\r\n                  >\r\n                    ลบ\r\n                  </a>\r\n                </a-menu-item>\r\n              </a-menu>\r\n            </a-dropdown>\r\n          </template>\r\n        </a-table>\r\n      </a-card>\r\n      <!-- / Projects Table Column -->\r\n    </div>\r\n\r\n    <!-- Modal Start Here -->\r\n    <a-modal\r\n      class=\"whole-modal-body\"\r\n      v-model=\"visible\"\r\n      title=\"Create Customer\"\r\n      @ok=\"handleOk\"\r\n    >\r\n      <div>\r\n        <div class=\"title-modal\">\r\n          <p>CUSTOMER</p>\r\n        </div>\r\n        <form @submit.prevent=\"submitForm()\" class=\"form-for-customer\">\r\n          <div class=\"toggle-type-customer\">\r\n            <div class=\"radio-selected\">\r\n              <input\r\n                v-model=\"status\"\r\n                id=\"individual\"\r\n                type=\"radio\"\r\n                value=\"บุคคลธรรมดา\"\r\n                @change=\"checkcompany(status)\"\r\n              />\r\n              <!-- บุคคลธรรมดา -->\r\n              <label for=\"individual\">บุคคลธรรมดา</label>\r\n            </div>\r\n            <div class=\"radio-selected\">\r\n              <input\r\n                v-model=\"status\"\r\n                id=\"juristic\"\r\n                type=\"radio\"\r\n                value=\"นิติบุคคล\"\r\n                @change=\"checkcompany(status)\"\r\n              />\r\n              <!-- นิติบุคคล -->\r\n              <label for=\"juristic\">นิติบุคคล</label>\r\n            </div>\r\n            <div class=\"error\" v-if=\"$v.status.$error\">\r\n              <template v-if=\"!$v.status.$invalid\"> </template>\r\n              <template v-else style=\"color: red\"> * </template>\r\n            </div>\r\n          </div>\r\n          <!-- /toggle-type-customer -->\r\n          <!-- ==================================================================== -->\r\n          <div v-if=\"status == 'นิติบุคคล'\">\r\n            <div class=\"contact-person-section\">\r\n              <p class=\"icon\" style=\"font-size: 18px; font-weight: 500\">\r\n                <b-icon icon=\"person\" style=\"color: #376303\"></b-icon>\r\n                ข้อมูลผู้ติดต่อ\r\n              </p>\r\n              <!-- Name of company -->\r\n              <div class=\"field\">\r\n                <input\r\n                  id=\"companyName\"\r\n                  type=\"text\"\r\n                  v-model=\"companyName\"\r\n                  placeholder=\"ชื่อบริษัท/ลูกค้า\"\r\n                  style=\"text-indent: 5%; border: 1px solid rgb(211, 211, 211)\"\r\n                />\r\n\r\n                <div class=\"error\" v-if=\"$v.companyName.$error\">\r\n                  <template v-if=\"!$v.companyName.$invalid\"> </template>\r\n                  <template v-else style=\"color: red\"> โปรดระบุชื่อ </template>\r\n                </div>\r\n              </div>\r\n              <!-- /Name of company -->\r\n              <div class=\"contact-person-section-buttom\">\r\n                <!-- COMPANY TEL. -->\r\n                <div class=\"field contact-person-section-buttom-part\">\r\n                  <input\r\n                    id=\"companyNumber\"\r\n                    type=\"tel\"\r\n                    v-model=\"companyNumber\"\r\n                    placeholder=\"เบอร์สำนักงาน\"\r\n                    style=\"\r\n                      text-indent: 10%;\r\n                      border: 1px solid rgb(211, 211, 211);\r\n                    \"\r\n                  />\r\n                  <div class=\"error\" v-if=\"$v.companyNumber.$error\">\r\n                    <template v-if=\"!$v.companyNumber.$invalid\"> </template>\r\n                    <template v-else-if=\"!$v.companyNumber.required\">\r\n                      โปรดใส่เบอร์บริษัท\r\n                    </template>\r\n                    <template v-else-if=\"!$v.companyNumber.validFormat\">\r\n                      เบอร์บริษัทต้องเป็นตัวเลข10หลัก\r\n                    </template>\r\n                  </div>\r\n                </div>\r\n                <!-- /COMPANY TEL. -->\r\n\r\n                <!-- TAX NUM -->\r\n                <div class=\"field contact-person-section-buttom-part\">\r\n                  <input\r\n                    id=\"taxNumber\"\r\n                    type=\"text\"\r\n                    v-model=\"taxNumber\"\r\n                    placeholder=\"เลขทะเบียนนิติบุคคล\"\r\n                    style=\"\r\n                      text-indent: 4%;\r\n                      border: 1px solid rgb(211, 211, 211);\r\n                    \"\r\n                  />\r\n                  <div class=\"error\" v-if=\"$v.taxNumber.$error\">\r\n                    <template v-if=\"!$v.taxNumber.$invalid\"> </template>\r\n                    <template v-else-if=\"!$v.taxNumber.required\">\r\n                      โปรดใส่หมายเลขกำกับภาษี</template\r\n                    >\r\n                    <template v-else-if=\"!$v.taxNumber.validFormat\">\r\n                      หมายเลขกำกับภาษีต้องเป็นตัวเลข13หลัก\r\n                    </template>\r\n                  </div>\r\n                </div>\r\n                <!-- TAX NUM -->\r\n              </div>\r\n            </div>\r\n            <!-- /contact-person-section -->\r\n          </div>\r\n          <!-- ==================================================================== -->\r\n          <!-- บุคคลธรรมดา -->\r\n          <div v-else>\r\n            <p class=\"icon\" style=\"font-size: 18px; font-weight: 500\">\r\n              <b-icon icon=\"person\" style=\"color: #376303\"></b-icon>\r\n              ข้อมูลผู้ติดต่อ\r\n            </p>\r\n            <div class=\"field\">\r\n              <!-- ชื่อผู้ติดต่อ -->\r\n              <div class=\"field\">\r\n                <input\r\n                  id=\"contactName\"\r\n                  type=\"text\"\r\n                  v-model=\"contactName\"\r\n                  placeholder=\"ชื่อบริษัท/ลูกค้า\"\r\n                  style=\"text-indent: 5%; border: 1px solid rgb(211, 211, 211)\"\r\n                />\r\n                <div class=\"error\" v-if=\"$v.contactName.$error\">\r\n                  <template v-if=\"!$v.contactName.$invalid\"> </template>\r\n                  <template v-else> โปรดระบุชื่อ </template>\r\n                </div>\r\n              </div>\r\n\r\n              <div class=\"contact-person-section-buttom\">\r\n                <div class=\"field contact-person-section-buttom-part\">\r\n                  <!-- เบอร์ผู้ติดต่อ -->\r\n\r\n                  <input\r\n                    id=\"contactNumber\"\r\n                    type=\"tel\"\r\n                    v-model=\"contactNumber\"\r\n                    style=\"\r\n                      text-indent: 10%;\r\n                      border: 1px solid rgb(211, 211, 211);\r\n                    \"\r\n                    placeholder=\"เบอร์โทร\"\r\n                  />\r\n                  <div class=\"error\" v-if=\"$v.contactNumber.$error\">\r\n                    <template v-if=\"!$v.contactNumber.$invalid\"> </template>\r\n                    <template v-else-if=\"!$v.contactNumber.required\">\r\n                      โปรดใส่เบอร์ผู้ติดต่อ\r\n                    </template>\r\n                    <template v-else-if=\"!$v.contactNumber.validFormat\">\r\n                      เบอร์ผู้ติดต่อต้องเป็นตัวเลข10หลัก\r\n                    </template>\r\n                  </div>\r\n                </div>\r\n                <!-- เลขกำกับภาษี -->\r\n                <div class=\"field contact-person-section-buttom-part\">\r\n                  <input\r\n                    id=\"taxNumber\"\r\n                    type=\"text\"\r\n                    v-model=\"taxNumber\"\r\n                    placeholder=\"เลขผู้เสียภาษี\"\r\n                    style=\"\r\n                      text-indent: 5%;\r\n                      border: 1px solid rgb(211, 211, 211);\r\n                    \"\r\n                  />\r\n                  <div class=\"error\" v-if=\"$v.taxNumber.$error\">\r\n                    <template v-if=\"!$v.taxNumber.$invalid\"> </template>\r\n                    <template v-else-if=\"!$v.taxNumber.required\">\r\n                      โปรดใส่หมายเลขกำกับภาษี</template\r\n                    >\r\n                    <template v-else-if=\"!$v.taxNumber.validFormat\">\r\n                      หมายเลขกำกับภาษีต้องเป็นตัวเลข13หลัก\r\n                    </template>\r\n                  </div>\r\n                </div>\r\n              </div>\r\n              <!-- email -->\r\n              <div class=\"field\">\r\n                <input\r\n                  id=\"contactEmail\"\r\n                  type=\"text\"\r\n                  v-model=\"contactEmail\"\r\n                  placeholder=\"อีเมล\"\r\n                  style=\"text-indent: 5%; border: 1px solid rgb(211, 211, 211)\"\r\n                />\r\n                <div class=\"error\" v-if=\"$v.contactEmail.$error\">\r\n                  <template v-if=\"!$v.contactEmail.$invalid\"> </template>\r\n                  <template v-else-if=\"!$v.contactEmail.required\">\r\n                    โปรดใส่อีเมล\r\n                  </template>\r\n                  <template v-else-if=\"!$v.contactEmail.email\">\r\n                    ใส่อีเมลให้ถูกต้อง\r\n                  </template>\r\n                </div>\r\n              </div>\r\n            </div>\r\n          </div>\r\n          <br />\r\n          <!-- / บุคคลธรรมดา -->\r\n          <div class=\"address-info\">\r\n            <p style=\"font-size: 18px; font-weight: 500\">\r\n              <b-icon icon=\"house-door\" style=\"color: #376303\"></b-icon>\r\n              ที่อยู่ผู้ติดต่อ\r\n            </p>\r\n            <!-- ที่อยู่ -->\r\n            <div class=\"addr-detail-content\">\r\n              <div class=\"addr-detail maxsize-input\">\r\n                <label for=\"Address\">ที่อยู่</label>\r\n                <input\r\n                  id=\"Address\"\r\n                  class=\"inputbox\"\r\n                  type=\"text\"\r\n                  v-model=\"Address\"\r\n                  placeholder=\"ที่อยู่\"\r\n                  style=\"text-indent: 5%\"\r\n                />\r\n                <div class=\"error\" v-if=\"$v.Address.$error\">\r\n                  <template v-if=\"!$v.Address.$invalid\"> </template>\r\n                  <template v-else> โปรดใส่ที่อยู่ </template>\r\n                </div>\r\n              </div>\r\n\r\n              <!-- ตำบล -->\r\n              <div class=\"addr-detail\" id=\"addr-box\">\r\n                <label for=\"subdis\">ตำบล</label>\r\n                <ThailandAutoComplete\r\n                  v-model=\"subdis\"\r\n                  class=\"autofilladdr\"\r\n                  id=\"subdis\"\r\n                  type=\"district\"\r\n                  @select=\"select\"\r\n                  placeholder=\"ตำบล...\"\r\n                />\r\n                <div class=\"error\" v-if=\"$v.subdis.$error\">\r\n                  <template v-if=\"!$v.subdis.$invalid\"> </template>\r\n                  <template v-else> โปรดใส่ตำบล</template>\r\n                </div>\r\n              </div>\r\n              <!-- อำเภอ -->\r\n              <div class=\"addr-detail\" id=\"addr-box\">\r\n                <label for=\"dis\">อำเภอ</label>\r\n                <ThailandAutoComplete\r\n                  v-model=\"dis\"\r\n                  type=\"amphoe\"\r\n                  class=\"autofilladdr\"\r\n                  id=\"subdis\"\r\n                  @select=\"select\"\r\n                  placeholder=\"อำเภอ...\"\r\n                />\r\n                <div class=\"error\" v-if=\"$v.dis.$error\">\r\n                  <template v-if=\"!$v.dis.$invalid\"> </template>\r\n                  <template v-else> โปรดใส่อำเภอ</template>\r\n                </div>\r\n              </div>\r\n              <!-- จังหวัด -->\r\n              <div class=\"addr-detail\" id=\"addr-box\">\r\n                <label for=\"provice\">จังหวัด</label>\r\n                <ThailandAutoComplete\r\n                  v-model=\"province\"\r\n                  class=\"autofilladdr\"\r\n                  type=\"province\"\r\n                  id=\"subdis\"\r\n                  @select=\"select\"\r\n                  color=\"#35495e\"\r\n                  placeholder=\"จังหวัด...\"\r\n                />\r\n                <div class=\"error\" v-if=\"$v.province.$error\">\r\n                  <template v-if=\"!$v.province.$invalid\"> </template>\r\n                  <template v-else> โปรดใส่จังหวัด</template>\r\n                </div>\r\n              </div>\r\n              <!-- zip code -->\r\n              <div class=\"addr-detail\" id=\"addr-box\">\r\n                <label for=\"postcode\">รหัสไปรษณีย์</label>\r\n                <ThailandAutoComplete\r\n                  v-model=\"postcode\"\r\n                  type=\"zipcode\"\r\n                  id=\"subddis\"\r\n                  class=\"autofilladdr\"\r\n                  @select=\"select\"\r\n                  color=\"#00a4e4\"\r\n                  placeholder=\"รหัสไปรษณีย์...\"\r\n                />\r\n                <div class=\"error\" v-if=\"$v.postcode.$error\">\r\n                  <template v-if=\"!$v.postcode.$invalid\"> </template>\r\n                  <template v-else> โปรดใส่รหัสไปรษณีย์</template>\r\n                </div>\r\n              </div>\r\n            </div>\r\n          </div>\r\n          <br />\r\n          <!-- รายละเอียดผู้ติดต่อ -->\r\n          <div v-if=\"status == 'นิติบุคคล'\">\r\n            <p style=\"font-size: 18px; font-weight: 500\">\r\n              <b-icon icon=\"person\" style=\"color: #376303\"></b-icon>\r\n              รายละเอียดผู้ติดต่อ\r\n            </p>\r\n\r\n            <div class=\"field\">\r\n              <label for=\"contactName\"> ชื่อผู้ติดต่อ</label>\r\n              <input\r\n                id=\"contactName\"\r\n                type=\"text\"\r\n                v-model=\"contactName\"\r\n                placeholder=\"ชื่อผู้ติดต่อ\"\r\n                style=\"\r\n                  text-indent: 5%;\r\n                  border: 1px solid rgb(211, 211, 211);\r\n                  opacity: 0.5;\r\n                \"\r\n              />\r\n              <div class=\"error\" v-if=\"$v.contactName.$error\">\r\n                <template v-if=\"!$v.contactName.$invalid\"> </template>\r\n                <template v-else> โปรดระบุชื่อ </template>\r\n              </div>\r\n            </div>\r\n\r\n            <div class=\"field\">\r\n              <label for=\"contactNumber\"> เบอร์โทร </label>\r\n              <input\r\n                id=\"contactNumber\"\r\n                type=\"text\"\r\n                v-model=\"contactNumber\"\r\n                placeholder=\"เบอร์โทร\"\r\n                style=\"\r\n                  text-indent: 5%;\r\n                  border: 1px solid rgb(211, 211, 211);\r\n                  opacity: 0.5;\r\n                \"\r\n              />\r\n              <div class=\"error\" v-if=\"$v.contactNumber.$error\">\r\n                <template v-if=\"!$v.contactNumber.$invalid\"> </template>\r\n                <template v-else-if=\"!$v.contactNumber.required\">\r\n                  โปรดใส่เบอร์ผู้ติดต่อ\r\n                </template>\r\n                <template v-else-if=\"!$v.contactNumber.validFormat\">\r\n                  เบอร์ผู้ติดต่อต้องเป็นตัวเลข10หลัก\r\n                </template>\r\n              </div>\r\n            </div>\r\n\r\n            <div class=\"field\">\r\n              <label for=\"contactEmail\"> อีเมล </label>\r\n              <input\r\n                id=\"contactEmail\"\r\n                type=\"text\"\r\n                v-model=\"contactEmail\"\r\n                placeholder=\"อีเมล\"\r\n                style=\"\r\n                  text-indent: 5%;\r\n                  border: 1px solid rgb(211, 211, 211);\r\n                  opacity: 0.5;\r\n                \"\r\n              />\r\n              <div class=\"error\" v-if=\"$v.contactEmail.$error\">\r\n                <template v-if=\"!$v.contactEmail.$invalid\"> </template>\r\n                <template v-else-if=\"!$v.contactEmail.required\">\r\n                  โปรดใส่อีเมล\r\n                </template>\r\n                <template v-else-if=\"!$v.contactEmail.email\">\r\n                  ใส่อีเมลให้ถูกต้อง\r\n                </template>\r\n              </div>\r\n            </div>\r\n          </div>\r\n          <div v-else></div>\r\n          <div class=\"submit-but-section\">\r\n            <button class=\"submit-button\" type=\"submit\">บันทึกข้อมูล</button>\r\n          </div>\r\n        </form>\r\n      </div>\r\n    </a-modal>\r\n\r\n    <!-- /Modal edit Start Here -->\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport {\r\n  required,\r\n  minLength,\r\n  maxLength,\r\n  alpha